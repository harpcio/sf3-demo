{% extends "@AceCommon/Default/layout.html.twig" %}

{% block content %}
    <div class="page-header">
        <h1>{{ 'Creating blog item' | trans }}</h1>
        <a href="{{ path('cms.blog.list') }}" class="btn btn-info">{{ 'Back to overview' | trans }}</a>
    </div>
    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.intro) }}
    {{ form_row(form.content) }}
    {{ form_row(form.events) }}
    {{ form_end(form) }}
{% endblock content %}